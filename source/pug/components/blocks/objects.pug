  section.page__objects.objects
    .container
      if (singleTitle)
          h1.objects__title Объекты
      else
          .objects__header
            h1.objects__title Наши объекты
            a.btn.btn--white(href="#", aria-label="Все объекты") Все объекты
      .objects__wrapper
        each item in objectItems
          .objects__item
              +object(item)
      if (hasPagination)
        .objects__pagination.pagination
          .pagination__button.pagination__button--start
          .pagination__button.pagination__button--prev
          ul.pagination__list.list-reset
            li.pagination__item.is-active
              a.pagination__link 1
            li.pagination__item
              a.pagination__link 2
            li.pagination__item
              a.pagination__link 3
            li.pagination__item
              a.pagination__link 4
            li.pagination__item
              a.pagination__link 5
            li.pagination__item
              a.pagination__link ...
            li.pagination__item
              a.pagination__link 46
          .pagination__button.pagination__button--next
          .pagination__button.pagination__button--end
      else
        .objects__advice.advice
          .advice__background
            .advice__content
              .advice__text Быстрый расчёт заявки! Хотите так же? Обращайтесь!
              a.advice__btn(href="#", aria-label="Получить консультацию")
               span Получить консультацию
            .advice__image
              img(src="img/content/advice/bg.png" width="" height="" alt="background")


